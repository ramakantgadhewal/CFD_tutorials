FoamFile
{
    version         2.0;
    format          ascii;
    class           volScalarField;
    object          He;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 0 0 0 0 0 0];

internalField   uniform 0.0;

boundaryField
{
    inlet
    {
        type                totalFlowRateAdvectiveDiffusive;
        massFluxFraction    1.0;
        phi                 phi;
        rho                 rho;
        value               uniform 1.0;
    }

    plate
    {
        type            zeroGradient;
    }

    coflow
    {
        type            inletOutlet;
        inletValue      $internalField;
        value           $internalField;
    }

    sides
    {
        type            inletOutlet;
        inletValue      $internalField;
        value           $internalField;
    }

    outlet
    {
        type            inletOutlet;
        inletValue      $internalField;
        value           $internalField;
    }
}

